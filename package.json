{
	"name": "my-snow-monkey-boilerplate",
	"version": "1.0.0",
	"scripts": {
		"start": "run-s start:{wp-env}",
		"start:wp-env": "wp-env start",
		"stop": "wp-env stop",
		"import-db": "wp-env run cli wp db import sql/env.sql",
		"export-db": "wp-env run cli wp db export sql/env.sql",
		"lint": "run-p lint:*",
		"lint:php": "composer lint",
		"fix": "run-s fix:{style,script,format,php}",
		"fix:php": "composer format"
	},
	"devDependencies": {
		"@wordpress/browserslist-config": "6.6.0",
		"@wordpress/env": "10.6.0",
		"@wordpress/prettier-config": "4.6.0",
		"@wordpress/stylelint-config": "22.6.0",
		"lefthook": "1.7.14",
		"npm-run-all2": "6.2.2",
		"stylelint": "^14.16.1",
		"stylelint-config-prettier": "9.0.5",
		"stylelint-config-recess-order": "3.1.0"
	},
	"volta": {
		"node": "20.17.0"
	}
}
